{
	"env": {
		"browser": true,
		"node": false
	},
	"globals": {
		// true=modifiable; false=var should never be written to
		"console": false,
		"$": false,
		"io": false,
		"UTIL": true
	},
	"rules": {
		// 0=off; 1=Warning; 2=Error
		"eol-last": 0,
		"no-use-before-define": 0,
		"quotes": 0,
		"no-unused-vars": [ 2, { "vars": "all", "args": "none" } ],
		"no-underscore-dangle": 0,
		"no-console": 1,
		"curly": 0,
        "no-trailing-spaces": 0
	}
}